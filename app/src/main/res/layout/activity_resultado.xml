<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.coltrack.codigopolicia.Resultado">

    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView15"
            android:layout_alignLeft="@+id/imageButton_atras"
            android:layout_alignStart="@+id/imageButton_atras"
            android:id="@+id/gridLayout3"
            android:layout_marginTop="5dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nombre: "
                android:id="@+id/textView16"
                android:layout_row="0"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="New Text"
                android:id="@+id/textView_nombre"
                android:layout_row="0"
                android:layout_column="1"
                android:lines="2"
                android:inputType="textMultiLine"
                android:maxLines="6"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Edad: "
                android:id="@+id/textViewEdad"
                android:layout_row="1"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="xx"
                android:id="@+id/textView_edad"
                android:layout_row="1"
                android:layout_column="1"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CÃ©dula: "
                android:id="@+id/textView17"
                android:layout_row="2"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ced"
                android:id="@+id/textView_cedula"
                android:layout_row="2"
                android:layout_column="1"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fecha de Nacimiento: "
                android:id="@+id/textView20"
                android:layout_row="3"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nac"
                android:id="@+id/textView_fechanacimiento"
                android:layout_row="3"
                android:layout_column="1"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sexo: "
                android:id="@+id/textView21"
                android:layout_row="4"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nac"
                android:id="@+id/textView_sexo"
                android:layout_row="4"
                android:layout_column="1"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Grupo Sanguineo: "
                android:id="@+id/textView22"
                android:layout_row="5"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nac"
                android:id="@+id/textView_grupoSanguineo"
                android:layout_row="5"
                android:layout_column="1"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Direccion GPS: "
                android:id="@+id/textView23"
                android:layout_row="6"
                android:layout_column="0"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nac"
                android:id="@+id/textView_direccion"
                android:layout_row="6"
                android:layout_column="1"
                android:lines="2"
                android:inputType="textMultiLine"
                android:maxLines="6"
                android:textSize="16dp" />
        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gridLayout4"
            android:layout_below="@+id/textView24"
            android:layout_centerHorizontal="true">

        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gridLayout5"
            android:layout_below="@+id/gridLayout4"
            android:layout_centerHorizontal="true">

        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gridLayout2"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="96dp">

        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="150dp">

        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true">

            <TextView

                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_column="1"
                android:layout_below="@+id/gridLayout3"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:textColor="#90c657"

                android:text="@string/button_interpol"
                android:id="@+id/textView_interpol"
                android:layout_marginLeft="50dp" />

            <TextView

                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/gridLayout8"
                android:layout_alignRight="@+id/gridLayout8"
                android:layout_alignEnd="@+id/gridLayout8"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textColor="#e45857"

                android:gravity="center"
                android:text="@string/button_dijin"
                android:id="@+id/textView_dijin" />

            <TextView

                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_below="@+id/gridLayout8"
                android:layout_alignRight="@+id/gridLayout8"
                android:layout_alignEnd="@+id/gridLayout8"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textColor="#ffffffff"
                android:background="@drawable/buttonshape"
                android:gravity="center"
                android:text="@string/button_finalizar"
                android:id="@+id/imageButton_finalizar"
                android:layout_marginTop="20dp" />

            <TextView

                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_below="@+id/gridLayout8"
                android:layout_alignRight="@+id/gridLayout8"
                android:layout_alignEnd="@+id/gridLayout8"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textColor="#ffffffff"
                android:background="@drawable/buttonshape"
                android:gravity="center"
                android:text="@string/button_medidas"
                android:id="@+id/imageButton_medidas"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="20dp" />


        </GridLayout>

    </RelativeLayout>
</RelativeLayout>
